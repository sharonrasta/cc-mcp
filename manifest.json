{
  "manifest_version": 3,
  "name": "Live Browser (CDP Only)",
  "version": "0.0.0",
  "description": "Capture console/errors/warnings via Chrome DevTools Protocol.",
  "permissions": ["debugger", "tabs", "storage"],
  "host_permissions": ["http://*/*", "https://*/*"],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "action": {
    "default_title": "Live Browser",
    "default_popup": "popup.html"
  }
}